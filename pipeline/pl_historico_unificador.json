{
	"name": "pl_historico_unificador",
	"properties": {
		"activities": [
			{
				"name": "Unificar_Parquet_1FL",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Shared/unificar_parquet_1FL",
					"baseParameters": {
						"filesystem": {
							"value": "@pipeline().parameters.filesystem",
							"type": "Expression"
						},
						"capa_raw": {
							"value": "@pipeline().parameters.capa_raw",
							"type": "Expression"
						},
						"rutaBase": {
							"value": "@pipeline().parameters.rutaBase",
							"type": "Expression"
						},
						"nombre_archivo": {
							"value": "@pipeline().parameters.nombre_archivo",
							"type": "Expression"
						},
						"historical_start_year": {
							"value": "@pipeline().parameters.historical_start_year",
							"type": "Expression"
						},
						"current_year": {
							"value": "@pipeline().parameters.current_year",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_databricks_kv_clusterparam",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "RegistrarRutaRaw_Historico",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "Unificar_Parquet_1FL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[sp_registrar_path]",
					"storedProcedureParameters": {
						"PipelineId": {
							"value": {
								"value": "@pipeline().parameters.PipelineId",
								"type": "Expression"
							},
							"type": "Expression"
						},
						"RunId": {
							"value": {
								"value": "@pipeline().RunId",
								"type": "Expression"
							},
							"type": "Expression"
						},
						"Entidad": {
							"value": {
								"value": "@pipeline().parameters.nombre_archivo",
								"type": "Expression"
							},
							"type": "Expression"
						},
						"ModoEjecucion": {
							"value": "'HISTORICO'",
							"type": "Expression"
						},
						"Periodo": {
							"value": {
								"value": "@concat(string(pipeline().parameters.historical_start_year), '-', string(pipeline().parameters.current_year))",
								"type": "Expression"
							},
							"type": "Expression"
						},
						"RutaRaw": {
							"value": {
								"value": "@concat(pipeline().parameters.capa_raw, trim(pipeline().parameters.rutaBase), '/', pipeline().parameters.nombre_archivo, '/1FL/data/', pipeline().parameters.nombre_archivo, '.parquet')",
								"type": "Expression"
							},
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_sql_liga1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Eliminar_Temporales",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "RegistrarRutaRaw_Historico",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.06:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Shared/eliminar_temporales_1FL",
					"baseParameters": {
						"filesystem": {
							"value": "@pipeline().parameters.filesystem",
							"type": "Expression"
						},
						"capa_raw": {
							"value": "@pipeline().parameters.capa_raw",
							"type": "Expression"
						},
						"rutaBase": {
							"value": "@pipeline().parameters.rutaBase",
							"type": "Expression"
						},
						"nombre_archivo": {
							"value": "@pipeline().parameters.nombre_archivo",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_databricks_kv_clusterparam",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"filesystem": {
				"type": "string"
			},
			"capa_raw": {
				"type": "string"
			},
			"rutaBase": {
				"type": "string"
			},
			"nombre_archivo": {
				"type": "string"
			},
			"PipelineId": {
				"type": "int"
			},
			"historical_start_year": {
				"type": "int"
			},
			"current_year": {
				"type": "int"
			}
		},
		"folder": {
			"name": "pl_hijos_ligaperuana/pl_control_historico"
		},
		"annotations": []
	}
}